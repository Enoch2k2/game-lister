<h1>Create Game</h1>

<%= form_for @game do |f| %>
  <div class="input-field">
    <%= f.text_field :name %>
    <%= f.label :name %>
  </div>
  <div class="input-field">
    <%= f.label :image_url %>
    <%= f.text_field :image_url %>
  </div>

  <%= f.submit "Create Game", class: "btn" %>
<% end %>